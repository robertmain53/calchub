#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/ipx@2.1.0_@netlify+blobs@8.2.0_db0@0.3.2_ioredis@5.6.1/node_modules/ipx/bin/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/ipx@2.1.0_@netlify+blobs@8.2.0_db0@0.3.2_ioredis@5.6.1/node_modules/ipx/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/ipx@2.1.0_@netlify+blobs@8.2.0_db0@0.3.2_ioredis@5.6.1/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/ipx@2.1.0_@netlify+blobs@8.2.0_db0@0.3.2_ioredis@5.6.1/node_modules/ipx/bin/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/ipx@2.1.0_@netlify+blobs@8.2.0_db0@0.3.2_ioredis@5.6.1/node_modules/ipx/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/ipx@2.1.0_@netlify+blobs@8.2.0_db0@0.3.2_ioredis@5.6.1/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ipx/bin/ipx.mjs" "$@"
else
  exec node  "$basedir/../ipx/bin/ipx.mjs" "$@"
fi
