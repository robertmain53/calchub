#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.1_jiti@2.4.2_terser@5.42.0_tsx@4.19.2__9c6843af5105e358db113b1add8de20f/node_modules/@nuxt/devtools/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.1_jiti@2.4.2_terser@5.42.0_tsx@4.19.2__9c6843af5105e358db113b1add8de20f/node_modules/@nuxt/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.1_jiti@2.4.2_terser@5.42.0_tsx@4.19.2__9c6843af5105e358db113b1add8de20f/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.1_jiti@2.4.2_terser@5.42.0_tsx@4.19.2__9c6843af5105e358db113b1add8de20f/node_modules/@nuxt/devtools/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.1_jiti@2.4.2_terser@5.42.0_tsx@4.19.2__9c6843af5105e358db113b1add8de20f/node_modules/@nuxt/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/@nuxt+devtools@2.5.0_vite@6.3.5_@types+node@24.0.1_jiti@2.4.2_terser@5.42.0_tsx@4.19.2__9c6843af5105e358db113b1add8de20f/node_modules:/home/yeahupsrl/socalsolver.com/calchub/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.mjs" "$@"
else
  exec node  "$basedir/../../cli.mjs" "$@"
fi
